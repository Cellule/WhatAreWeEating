    "express": "^4.16.4",
    "mongodb": "^3.1.10",
    "mongoose": "^5.4.2",
    "typescript": "3.2.2"
  },
  "devDependencies": {
    "concurrently": "^4.0.1",
    "nodemon": "^1.18.9"
  }
}
